                                 img_fol_t *img_fol, raw_cparameters_t *raw_cp, char *indexfilename,
                                 size_t indexfilename_size,
                                 int* pOutFramerate,
                                 OPJ_BOOL* pOutPLT,
                                 OPJ_BOOL* pOutTLM,
