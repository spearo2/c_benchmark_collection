        {"TP", 1, ((void*)0), 'u'},
        {"SOP", 0, ((void*)0), 'S'},
        {"EPH", 0, ((void*)0), 'E'},
        {"OutFor", 1, ((void*)0), 'O'},
        {"POC", 1, ((void*)0), 'P'},
        {"ROI", 1, ((void*)0), 'R'},
        {"jpip", 0, ((void*)0), 'J'},
        {"mct", 1, ((void*)0), 'Y'},
