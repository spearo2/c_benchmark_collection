    height = (uint32_t)image->comps[0].h;

    TIFFSetField(tif, 256, width);
    TIFFSetField(tif, 257, height);
    TIFFSetField(tif, 277, (uint16_t)numcomps);
    TIFFSetField(tif, 258, bps);
    TIFFSetField(tif, 274, 1);
    TIFFSetField(tif, 284, 1);
    TIFFSetField(tif, 262, tiPhoto);
    TIFFSetField(tif, 278, 1);
